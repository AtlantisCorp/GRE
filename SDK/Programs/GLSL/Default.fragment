// A basic Fragment shader.
#version 330
#include Programs/GLSL/BaseConstants.glsl

in VertexOutput vertex_output ;
out vec4 oFragment ;

#include Programs/GLSL/BaseIncludes.glsl

uniform sampler2D iTexture0 ;

void main ()
{
    oFragment = LightsColor() * texture( iTexture0 , vertex_output.texcoord ) ;
}